{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.incture.project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.5",
      "toolsId": "87d4a29b-c4f6-4818-93d5-6e16e5abfe7d"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.124.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.incture.project1.i18n.i18n"
        }
      },
    
      "myModel":{
        "type":"sap.ui.model.json.JSONModel",
        "uri":"model/myModel.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.incture.project1.view",
        "controlAggregation": "pages",
        "controlId": "idApp"
      },
      "routes": [
        {
          "name": "firstroot",
          "pattern": "",
          "target": [
            "firstarget"
          ]
        },
        {
          "name": "secondroot",
          "pattern": "secondview",
          "target": [
            "secondtarget"
          ]
        },
        {
          "name":"thirdroot",
          "pattern": "thirdview",
          "target":[
            "thirdtarget"
          ]
        },
        {
          "name": "fourthroot",
          "pattern": "fourthview",
          "target":[
            "fourthtarget"
          ]
        },
        {
          "name":"layoutroot",
          "pattern": "layoutview",
          "target":[
            "layouttarget"
          ]
        },
        {
          "name":"simpleformroot",
          "pattern": "simpleform",
          "target":[
            "simpleformtarget"
          ]
        },
        {
          "name":"previewroot",
          "pattern": "preview",
          "target":[
            "previewapp"
          ]
        },{
          "name":"bindingroot",
          "pattern": "binding",
          "target":[
            "bindingapp"
          ]
        },{
          "name":"bindingresultroot",
          "pattern": "bindingresult/{Index}",
          "target":[
            "bindingtarget"
          ]
        }


      ],
      "targets": {
        "firstarget": { 
          "viewId": "idfirst",
          "viewName": "first",
          "viewLevel": 1
        },
        "secondtarget": {       
          "viewId": "idsecond",
          "viewName": "second",
          "viewLevel": 2
        },
        "thirdtarget":{
          "viewId": "idthird",
          "viewName": "third",
          "viewLevel": 3
        },
        "fourthtarget":{
          "viewId": "idfourth",
          "viewName": "fourth",
          "viewLevel": 4
        },
        "layouttarget":{
          "viewId": "idlayout",
          "viewName": "layout",
          "viewLevel": 5
        },
        "simpleformtarget":{
          "viewId": "idsimpleform",
          "viewName": "simpleformlayout",
          "viewLevel": 6
        },
        "previewapp":{
          "viewId": "preview",
          "viewName": "preview",
          "viewLevel": 7

      },
      "bindingapp":{
        "viewId": "binding",
        "viewName": "binding",
        "viewLevel": 8
      },
      "bindingtarget":{
        "viewId": "bindingresult",
        "viewName": "bindingresults",
        "viewLevel": 9
      }

      }
   },
    "rootView": {
      "viewName": "com.incture.project1.view.App",
      "type": "XML",
      "id": "idApp"
    }
  }
}
